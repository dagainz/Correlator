{
  "system": {
    "config": {
     "system.run_dir": "/Users/timp/Projects/Correlator/run"
    }
  },
  "application": {
    "report": {
      "modules" : [
        ["Correlator.Module.report", "Report"]
      ],
      "handlers": {
        "Logback": {
          "handler": ["Correlator.Event.log", "LogbackListener"],
          "filter_expression": "${event.status in [EventStatus.Warning, EventStatus.Error]}"
        }
      },
      "config": {
          "setting": "value"
        }
    },
    "siem-report": {
      "modules" : [
        ["Correlator.Module.sshd", "SSHD"]
      ],
      "handlers": {
        "Logback": {
          "handler": ["Correlator.Event.log", "LogbackListener"],
          "filter_expression": "${event.status in [EventStatus.Warning, EventStatus.Error]}"
        }
      },
      "config": {
        "module.sshd.login_failure_limit": "6"
      }
    }
  }
}


